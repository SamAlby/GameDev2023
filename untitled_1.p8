pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
--draw

function _update()
if time()-anim_time>anim_wait then
--first quadrant
anim_time=time()
 frog_sprite=hand_nums[hand_nums_index]
	hand_nums_index += 1
	if hand_nums_index>12 then
		hand_nums_index=1
		end
		
	 
--second quadrant
	sec_h=hand_nums[hand_nums_index]+1
	hand_nums_index += 1
	if hand_nums_index>12 then
		hand_nums_index=1
		end
		
--third quadrant

thi_h=hand_nums[hand_nums_index]+16
	hand_nums_index += 1
	if hand_nums_index>12 then
		hand_nums_index=1
		end

--fourth quadrant		
fou_h=hand_nums[hand_nums_index]+17
	hand_nums_index += 1
	if hand_nums_index>12 then
		hand_nums_index=1
		
		end		


end

print(time())
		
end

function _draw()
	cls()`
	spr(frog_sprite, frog_x, frog_y)
	spr(sec_h, sec_x, sec_y)
	spr(thi_h, thi_x, thi_y)
	spr(fou_h, fou_x, fou_y)
end	
-->8
--variables
anim_time=0
anim_wait=0.25

frog_sprite=1
frog_x=60
frog_y=60

sec_x=frog_x+8
sec_y=frog_y

thi_x=frog_x
thi_y=frog_y+8

fou_x=frog_x+8
fou_y=frog_y+8

--first quadrant variables
hand_nums={80,62,60,58,56,54,52,50,48,13,11,9}
hand_nums_index=1
frog_sprite=hand_nums[hand_nums_index]
--second quadrant variables
sec_h = hand_nums[hand_nums_index]+1
--third quadrant
thi_h = hand_nums[hand_nums_index]+16
--fourth quadrant
fou_h= hand_nums[hand_nums_index]+17



